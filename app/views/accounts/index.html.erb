 <div class="container">
<p id="notice"><%= notice %></p>

<u><h1>Listing Accounts</h1></u>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Account no</th>
      <th>Balance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.account_no %></td>
        <td><%= account.balance %></td>
        <td><%= link_to 'Show', account, class:"btn btn-success" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'Back', atm_machines_path, class:"btn btn-link" %>
</div>

