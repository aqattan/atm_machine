 <div class="container">
<p id="notice"><%= notice %></p>

<u><h1>Listing User Information</h1></u>

<table class="table table-striped">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_infos.each do |user_info| %>
      <tr>
        <td><%= user_info.first_name %></td>
        <td><%= user_info.last_name %></td>
        <td><%= user_info.phone %></td>
        <td><%= user_info.email %></td>
        <td><%= link_to 'Show', user_info, class:"btn btn-success" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%=link_to 'Back', atm_machine_path(session[:my_atm_machine]), class:"btn btn-link"%>
</div>
